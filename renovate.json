{
  "automerge": true,
  "commitMessage": "{{semanticPrefix}}Update {{depName}} to {{newVersion}} ðŸŒŸ",
  "extends": [
    "config:base"
  ],
  "labels": [
    "type: dependencies",
    "renovate"
  ],
  "lockFileMaintenance": {
    "enabled": true
  },
  "major": {
    "automerge": false
  },
  "masterIssue": true,
  "packageRules": [
    {
      "groupName": "eslint",
      "packagePatterns": "^eslint"
    },
    {
      "groupName": "typescript",
      "packageNames": [
        "@babel/preset-typescript",
        "tslint",
        "typescript"
      ]
    }
  ],
  "prConcurrentLimit": 3,
  "prHourlyLimit": 2,
  "prTitle": "{{semanticPrefix}}{{#if isPin}}Pin{{else}}Update{{/if}} dependency {{depName}} to version {{newVersion}} ðŸŒŸ",
  "rangeStrategy": "pin",
  "schedule": [
    "after 2am and before 3am on sunday"
  ],
  "separateMultipleMajor": true,
  "separatePatchReleases": true,
  "timezone": "America/New_York",
  "updateNotScheduled": false
}